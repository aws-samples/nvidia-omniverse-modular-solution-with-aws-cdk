{
  "name": "omni",
  "env": {
    "account": "ACCOUNT_ID",
    "region": "AWS_REGION"
  },
  "availabilityZones": 2,
  "removalPolicy": "destroy",
  "autoDelete": true,
  "cdkNag": false,
  "stacks": {
    "vpc": {
      "name": "vpc",
      "allowedRanges": ["CIDR_RANGE"]
    },
    "workstation": {
      "name": "workstation",
      "jumpboxInstanceType": "t4g.small",
      "workstationInstanceType": "g5.2xlarge",
      "workstationQuantity": 1
    }
  }
}
